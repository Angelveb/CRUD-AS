<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jovanovic.stefan.TrabajoInventario">

    <application
        android:allowBackup="true"
        android:icon="@drawable/starbuckslogo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/starbuckslogo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.jovanovic.stefan.TrabajoInventario.register"
            android:exported="false" />
        <activity
            android:name="com.jovanovic.stefan.TrabajoInventario.login"
            android:exported="true"
            android:label="Inventario">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.jovanovic.stefan.TrabajoInventario.UpdateActivity"
            android:parentActivityName="com.jovanovic.stefan.TrabajoInventario.MainActivity" />
        <activity
            android:name="com.jovanovic.stefan.TrabajoInventario.AddActivity"
            android:label="Agregar producto"
            android:parentActivityName="com.jovanovic.stefan.TrabajoInventario.MainActivity" />
        <activity
            android:name="com.jovanovic.stefan.TrabajoInventario.MainActivity"
            android:label="Inventario"></activity>
    </application>

</manifest>